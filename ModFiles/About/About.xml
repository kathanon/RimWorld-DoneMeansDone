<ModMetaData>
	<name>Don't Say 100% Unless You Mean It!</name>
	<author>kathanon</author>
    <packageId>kathanon.DoneMeansDone</packageId>
	<supportedVersions>
		<li>1.4</li>
	</supportedVersions>
	<description>When the game displays percentages, this mod prevents it from rounding up to 100. (Only showing "100%" when it is actually done.)</description>
	<url>https://github.com/kathanon/RimWorld-DoneMeansDone</url>

	<modDependencies>
		<li>
			<packageId>brrainz.harmony</packageId> 
			<displayName>Harmony</displayName>
			<downloadUrl>https://github.com/pardeike/HarmonyRimWorld/releases/latest</downloadUrl>
			<steamWorkshopUrl>steam://url/CommunityFilePage/2009463077</steamWorkshopUrl>
		</li>
	</modDependencies>
	<loadAfter>
		<li>brrainz.harmony</li>
	</loadAfter>
</ModMetaData>
